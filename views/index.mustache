<body>
  <div id="wrapper">
    <header>
      <h1>
        <a href="https://www.codeship.io">
          <img src="assets/codeship_santa_claus.png" alt="Codeship Christmas Logo">
        </a>
        Advent Calendar
      </h1>
    </header>
    <div id="today">
      {{#today}}
      <a href="{{link}}">
        <div class="day">{{day}}</div>
        <div class="description">{{description}}</div>
      </a>
      <p class="call_to_action">
        <a href="{{link}}">{{callToAction}}</a>
      </p>
      {{/today}}
    </div>
    <div id="days">
      <ul id="days_list">
        {{#days}}<li>
          {{#open}}
          <a href="{{link}}">
            <div class="day">{{day}}</div>
            <div class="title">{{title}}</div>
          </a>
          {{/open}}
          {{^open}}
          <div title="Coming soonâ€¦" class="day soon">{{day}}</div>
          {{/open}}
        </li>{{/days}}
      </ul>
    </div>
  </div>
